{
  "name": "@endo/daemonmask",
  "version": "0.0.0",
  "private": true,
  "description": "DaemonMask",
  "keywords": [
    "endo",
    "daemon",
    "metamask"
  ],
  "homepage": "https://github.com/endojs/endo/blob/master/packages/daemonmask/README.md",
  "bugs": {
    "url": "https://github.com/endojs/endo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/endojs/endo.git",
    "directory": "packages/daemonmask"
  },
  "license": "Apache-2.0",
  "author": "Endo contributors",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./src/index.js",
  "module": "./src/index.js",
  "files": [
    "LICENSE*",
    "SECURITY*",
    "src",
    "*.js",
    "*.ts"
  ],
  "scripts": {
    "build": "exit 0",
    "lint": "yarn lint:eslint && yarn lint:misc --check && yarn lint:dependencies --check && yarn lint:changelog",
    "lint:dependencies": "depcheck && yarn dedupe",
    "lint:eslint": "eslint . --cache --ext js,cjs,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write && yarn lint:dependencies && yarn lint:changelog",
    "lint:misc": "prettier '**/*.json' '**/*.md' '**/*.yml' '!.yarnrc.yml' --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "test": "ava",
    "test:coverage": "c8 ava"
  },
  "ava": {
    "files": [
      "src/**/*.test.*"
    ],
    "timeout": "2m"
  },
  "dependencies": {
    "@endo/daemon": "workspace:^",
    "@endo/exo": "workspace:^",
    "@endo/far": "workspace:^",
    "@endo/patterns": "workspace:^",
    "@endo/promise-kit": "workspace:^",
    "@ethereumjs/tx": "^4.2.0",
    "@metamask/eth-hd-keyring": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@endo/ses-ava": "^1.2.2",
    "@metamask/eslint-config": "^12.2.0",
    "@metamask/eslint-config-browser": "^12.1.0",
    "@metamask/eslint-config-nodejs": "^12.1.0",
    "@metamask/eslint-config-typescript": "^12.1.0",
    "@types/node": "^16",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "ava": "^6.1.3",
    "c8": "^7.14.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "typescript": "5.5.0-beta"
  }
}
